<div class="<%= singular_name %>">
<% attributes.each do |attribute| -%>
	<p><b><%= attribute.column.human_name %>:</b> <%%=h @<%= singular_name %>.<%= attribute.name %> %></p>
<% end -%>
</div>

<%%= link_to t('<%= singular_name %>.edit', :default => :'model.edit'), edit_<%= singular_name %>_path(@<%= singular_name %>) %> | <%%= link_to t('<%= singular_name %>.list', :default => :'model.list'), <%= plural_name %>_path %>
